    "paths": {
      "/api/atms": {
        "get": {
          "summary": "Get all ATMs",
          "responses": {
            "200": {
              "description": "Successful response",
              "content": {
                "application/json": {
                  "example": {
                    "atms": []
                  }
                }
              }
            }
          }
        },
        "post": {
          "summary": "Create a new ATM",
          "requestBody": {
            "description": "ATM details",
            "content": {
              "application/json": {
                "example": {
                  "key1": "value1",
                  "key2": "value2"
                }
              }
            }
          },
          "responses": {
            "201": {
              "description": "ATM created successfully",
              "content": {
                "application/json": {
                  "example": {
