          "responses": {
            "200": {
              "description": "Successful response",
              "content": {
                "application/json": {
                  "example": {
                    "Accessibility": ["WheelchairAccess", "AudioCashMachine"],
                    "ATMServices": ["CashWithdrawal", "ChequeDeposits"],
                    "Access24HoursIndicator": false
                  }
